script/source = "extends Control


# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"
onready var player = get_node(\"Player\")

onready var panel = get_node(\"Panel\")
onready var monitor = get_node(\"Monitor\")
onready var door_hall = get_node(\"Hall_door\")
onready var door_director = get_node(\"Director_door\")

# Called when the node enters the scene tree for the first time.
func _ready():
	player.position.x = 160
	player.get_node(\"Sprite\").flip_h = false
	pass # Replace with function body.


func _on_Panel_pressed():
	player.move_to_object(panel)


func _on_Monitor_pressed():
	player.move_to_object(monitor)


func _on_Hall_door_pressed():
	player.move_to_object(door_hall)


func _on_Director_door_pressed():
	player.move_to_object(door_director)
"

